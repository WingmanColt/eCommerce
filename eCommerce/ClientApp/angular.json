              "node_modules/font-awesome/scss/font-awesome.scss",
              "projects/user/src/assets/scss/app.scss",
              "projects/user/src/assets/scss/themify.scss",
              "projects/user/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
