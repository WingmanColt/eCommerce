            ],
            "styles": [
              "node_modules/font-awesome/scss/font-awesome.scss",
              "projects/user/src/assets/scss/app.scss",
              "projects/user/src/assets/scss/themify.scss",
              "projects/user/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "UI-shared": {
      "projectType": "library",
      "root": "projects/ui-shared",
      "sourceRoot": "projects/ui-shared/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ui-shared/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ui-shared/tsconfig.lib.prod.json"
            },
            "development": {
