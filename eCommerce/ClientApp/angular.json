            "tsConfig": "projects/core-shared/tsconfig.spec.json",
            "karmaConfig": "projects/core-shared/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "191e567d-172e-4069-b361-e8113fb5eb6f"
  }
}
